{
    "env": {
        "browser": true,
        "es2021": true,
        "webextensions": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "func-names": ["error", "as-needed"],
        "no-console": "off",
        "no-undef": "off",
        "no-alert": "off"
    }
}
